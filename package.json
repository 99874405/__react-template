{
  "private": true,
  "author": "我亦无他",
  "scripts": {
    "dev": "cross-env UMI_ENV=~dev umi dev",
    "build": "cross-env UMI_ENV=~prod umi build",
    "build:test1": "cross-env UMI_ENV=~test1 umi build",
    "build:test2": "cross-env UMI_ENV=~test2 umi build",
    "build:test3": "cross-env UMI_ENV=~test3 umi build",
    "setup": "umi setup",
    "postinstall": "umi setup",
    "push": "git add . && git-cz && git push",
    "prepare": "husky install"
  },
  "dependencies": {
    "tailwindcss": "^3",
    "umi": "^4.0.22"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/lint": "^4.0.22",
    "@umijs/plugins": "^4.0.22",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "stylelint": "^14.12.1",
    "stylelint-config-prettier": "^9.0.3",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,less}": [
      "umi lint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
